{
    "extends": ["hexo/test", "plugin:jest/recommended"],
    "plugins": ["jest"],
    "env": {
        "mocha": false,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "space-before-function-paren": ["error", {
            "named": "always",
            "asyncArrow": "always"
        }]
    }
}